require LocationHelper
<div id="map">
  <script type="text/javascript">
    initMap();
  </script>
</div>
<script async defer
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyApzU3b0GAQzSzAUT9wtDNZtTZXi9WXi9Q&callback=initMap">
</script>

<script src="/jpeg_camera/swfobject.min.js" type="text/javascript"></script>
<script src="/jpeg_camera/canvas-to-blob.min.js" type="text/javascript"></script>
<script src="/jpeg_camera/jpeg_camera.min.js" type="text/javascript"></script>

<div class="container-fluid">
<div id="form">
<h4 id="time11"></h4>
<%= form_for(@location) do  |f| %>
  <h1>Location:</h1>
  <%=f.label :latitude %><br>
  <p>
  <%=f.number_field :latitude, :id=>"latitude", step: :any %>
  </p>
  <%=f.label :longitude %><br>
  <p>
  <%=f.number_field :longitude, :id=>"longitude", step: :any %>
  </p>
  <%=f.label :info %><br>
  <p>
  <%=f.text_area :info, rows: "4", cols: "50" %>
  </p>
  <%=f.submit "Save" %>
  <% end %>
</div>
</div>
        