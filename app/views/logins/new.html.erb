<%= render 'shared/page_title', title: 'Log in', :id => 'login' %>
<div id="login">
<div class="row">
<div class="col-md-5" >
    <%= form_tag '/login' do %>

      <%= label_tag :email %><br>
      <%= text_field_tag :email %>

      <%= label_tag :password %>
      <%= password_field_tag :password %>
      
      <%= submit_tag 'Login', class: 'btn btn-success' %>

    <% end %>
  </div>
</div>
</div>